{
  "name": "telegram-bot-sdk/core",
  "description": "The Telegram Bot API PHP SDK",
  "keywords": ["telegram", "telegram bot", "telegram bot api", "telegram sdk", "telegram php", "laravel telegram", "laravel"],
  "type": "library",
  "homepage": "https://github.com/telegram-bot-sdk/core",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Syed Irfaq R.",
      "email": "syed+gh@lukonet.com",
      "homepage": "https://github.com/irazasyed"
    }
  ],
  "require": {
    "php": "^7.4",
    "guzzlehttp/guzzle": "^6.3 || 7.0",
    "guzzlehttp/psr7": "^1.3",
    "illuminate/support": "^6.0 || ^7.0",
    "league/event": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Telegram\\Bot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Telegram\\Bot\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "illuminate/container": "Hold dependencies to be injected in commands constructors"
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
