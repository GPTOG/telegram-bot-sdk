{
  "name": "telegram-bot-api/php-sdk",
  "type": "library",
  "description": "The Telegram Bot API PHP SDK",
  "keywords": [
    "telegram",
    "chatbot",
    "telegram php",
    "telegram bot",
    "telegram chatbot",
    "telegram bot api",
    "telegram bot sdk",
    "telegram bot php sdk",
    "telegram bot api php sdk"
  ],
  "homepage": "https://github.com/telegram-bot-api/php-sdk",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Syed Irfaq R.",
      "email": "syed+gh@lukonet.com",
      "homepage": "https://github.com/irazasyed",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4",
    "guzzlehttp/guzzle": "^6.3 || 7.0",
    "guzzlehttp/psr7": "^1.3",
    "illuminate/support": "^6.0 || ^7.0",
    "league/event": "^2.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.3",
    "phpunit/phpunit": ">=8.5"
  },
  "suggest": {
    "illuminate/container": "Hold dependencies to be injected in commands constructors",
    "telegram-bot-api/laravel": "Install the Laravel service provider if you're using the SDK in Laravel"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Telegram\\Bot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Telegram\\Bot\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  }
}
